{
  "name": "GitHub Readme TOC",
  "description": "View the table of contents for the README file on GitHub's right side",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["scripts/index.js"]
    }
  ]
}
